{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "routes": [
    { "src": "/api/blog/(.*)", "dest": "/api/blog?id=$1" },
    { "src": "/api/docs/(.*)", "dest": "/api/docs?id=$1" },
    { "src": "/api/releases/(.*)", "dest": "/api/releases?id=$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
